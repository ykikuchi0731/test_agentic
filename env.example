# ServiceNow Configuration
SERVICENOW_INSTANCE=your-instance.service-now.com
SERVICENOW_USERNAME=your-username
SERVICENOW_PASSWORD=your-password

# API Settings
API_TIMEOUT=30
MAX_RETRIES=3

# Download Settings
DOWNLOAD_DIR=./downloads
MAX_FILE_SIZE=104857600

# Google Docs Export Settings
# Timeout for browser operations (page load, element wait) in seconds
GOOGLE_DOCS_BROWSER_TIMEOUT=60
# Timeout for file download completion in seconds
GOOGLE_DOCS_DOWNLOAD_TIMEOUT=120

# Notion Configuration
NOTION_API_KEY=your-notion-api-key
NOTION_DATABASE_ID=your-database-id
